{
  "name": "tictactoe",
  "version": "1.0.3",
  "description": "Tictactoe game for tutorial purpose",
  "main": "webpack.config.js",
  "scripts": {
    "test": "jest",
    "test:e2e": "jest && npm run e2e",
    "e2e": "npm-run-all -p -r webdriver:start webpack-dev-server protractor",
    "postinstall": "npm run webdriver:update",
    "protractor": "sleep 4 && protractor src/e2e/protractor.conf.js",
    "webpack-dev-server": "webpack-dev-server",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "webdriver-manager update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heroinedor/tictactoe.git"
  },
  "author": "Pipo Bimbo",
  "license": "",
  "bugs": {
    "url": "https://github.com/heroinedor/tictactoe/issues"
  },
  "publishConfig": {
    "registry": "http://localhost:4873"
  },
  "homepage": "https://github.com/heroinedor/tictactoe#readme",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.27.1",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^7.0.1",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "jest": "^19.0.2",
    "npm-run-all": "^4.0.2",
    "protractor": "^5.1.1",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.13.2",
    "webdriver-manager": "^12.0.3",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
}
